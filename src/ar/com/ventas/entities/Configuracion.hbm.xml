<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ar.com.ventas.entities.Configuracion" schema="ventas" table="configuracion">
        <id name="id" column="ID">
            <generator class="identity"/>
        </id>
        <property name="iva" type="float" column="IVA"/>
        <property name="sucursalA" type="integer" column="SUCURSAL_A"/>
        <property name="numeroFacturaA" type="integer" column="NUMERO_FACTURA_A"/>
        <property name="sucursalB" type="integer" column="SUCURSAL_B"/>
        <property name="numeroFacturaB" type="integer" column="NUMERO_FACTURA_B"/>
        <property name="numeroRecibo" type="integer" column="NUMERO_RECIBO"/>
        <property name="numeroPedido" type="integer" column="NUMERO_PEDIDO"/>
        <property name="cantidadFacturasA" type="integer" column="CANTIDAD_FC_A"/>
        <property name="cantidadFacturasB" type="integer" column="CANTIDAD_FC_B"/>
        <property name="cantidadPedidos" type="integer" column="CANTIDAD_PEDIDOS"/>
        <property name="cantidadNotasCredito" type="integer" column="CANTIDAD_NOTAS_CREDITO"/>
        <property name="cantidadRecibos" type="integer" column="CANTIDAD_RECIBOS"/>
        <property name="ultimaFechaCierre" type="date" column="ULTIMA_FECHA_CIERRE"/>
        <property name="ultimaFechaSistema" type="date" column="ULTIMA_FECHA_SISTEMA"/>
        <property name="cajaAnteriorCerrada" type="boolean" column="CAJA_ANTERIOR_CERRADA"/>
        <property name="numeroReciboCompras" type="integer" column="NUMERO_RECIBO_COMPRAS"/>
        <property name="comprobante" type="long" column="COMPROBANTE"/>
    </class>
</hibernate-mapping>